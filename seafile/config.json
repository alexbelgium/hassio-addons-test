{
  "arch": [
    "aarch64",
    "amd64"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "High performance file syncing and sharing, with also Markdown WYSIWYG editing, Wiki, file label and other knowledge management features",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {},
  "map": [
    "media:rw",
    "share:rw",
    "config:rw",
    "ssl"
  ],
  "name": "Seafile",
  "options": {
    "data_location": "/share/seafile",
    "seafile_server_letsencrypt": false,
    "seafile_server_hostname": "seafile.example.com",
    "seafile_admin_email": "me@example.com",
    "seafile_admin_password": "a_very_secret_password"
  },
  "panel_icon": "mdi:library",
  "ports": {
    "80/tcp": 8077
  },
  "ports_description": {
    "80/tcp": "seafile webui"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH",
    "NET_ADMIN"
  ],
  "schema": {
    "data_location": "str",
    "seafile_server_letsencrypt": "bool?",
    "seafile_server_hostname": "str",
    "seafile_admin_email": "email",
    "seafile_admin_password": "str",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "services": [
    "mysql:want"
  ],
  "slug": "seafile",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/seafile",
  "version": "0.6.18-ls164"
}
