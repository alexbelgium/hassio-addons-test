{
  "arch": [
    "aarch64",
    "amd64"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Mealie is a self hosted recipe manager and meal planner built in Vue",
  "devices": [
    "/dev/dri",
    "/dev/dri/card0",
    "/dev/dri/card1",
    "/dev/dri/renderD128",
    "/dev/vchiq",
    "/dev/video10",
    "/dev/video11",
    "/dev/video12",
    "/dev/video13",
    "/dev/video14",
    "/dev/video15",
    "/dev/video16",
    "/dev/ttyUSB0",
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/nvme0n1p3",
    "/dev/nvme1n1p1",
    "/dev/nvme1n1p2",
    "/dev/nvme1n1p3",
    "/dev/nvme2n1p1",
    "/dev/nvme2n1p2",
    "/dev/nvme3n1p3",
    "/dev/mmcblk",
    "/dev/fuse",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/sda5",
    "/dev/sda6",
    "/dev/sda7",
    "/dev/sda8",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "DATA_DIR": "/config/addons_config/mealie_data",
    "DB_ENGINE": "sqlite",
    "MAX_WORKERS": "1",
    "WEB_CONCURRENCY": "2",
    "WEB_GUNICORN": "true",
    "WORKERS_PER_CORE": "0.5"
  },
  "ingress": true,  
  "map": [
    "config:rw",
    "share:rw",
    "ssl:rw"
  ],
  "name": "Mealie",
  "options": {
    "ALLOW_SIGNUP": true,
    "PGID": 1000,
    "PUID": 1000,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "ssl": false
  },
  "panel_icon": "mdi:silverware-fork-knife",
  "ports": {
    "9000/tcp": 9090,
    "9001/tcp": 9091
  },
  "ports_description": {
    "9000/tcp": "Web interface",
    "9001/tcp": "Ssl web interface"
  },
  "schema": {
    "ALLOW_SIGNUP": "bool",
    "BASE_URL": "str?",
    "PGID": "int",
    "PUID": "int",
    "certfile": "str",
    "keyfile": "str",
    "ssl": "bool"
  },
  "slug": "mealie_test",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "v1.0.0-3",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:9000]"
}
