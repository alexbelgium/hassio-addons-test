{
  "arch": [
    "aarch64",
    "amd64"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Mealie is a self hosted recipe manager and meal planner built in Vue",
  "devices": [
    "/dev/dri",
    "/dev/dri/card0",
    "/dev/dri/card1",
    "/dev/dri/renderD128",
    "/dev/vchiq",
    "/dev/video10",
    "/dev/video11",
    "/dev/video12",
    "/dev/video13",
    "/dev/video14",
    "/dev/video15",
    "/dev/video16",
    "/dev/ttyUSB0",
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/fuse",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "DB_ENGINE": "sqlite",
    "WEB_GUNICORN": "true"
  },
  "ingress": true,
  "ingress_stream": true,
  "map": [
    "config:rw",
    "share:rw",
    "ssl:rw"
  ],
  "name": "Mealie (Omni):",
  "options": {
    "ALLOW_SIGNUP": true,
    "PGID": 0,
    "PUID": 0,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "ssl": false
  },
  "panel_icon": "mdi:silverware-fork-knife",
  "ports": {
    "3000/tcp": 9090,
    "9000/tcp": null
  },
  "ports_description": {
    "3000/tcp": "Web interface",
    "9000/tcp": "API port"
  },
  "schema": {
    "ALLOW_SIGNUP": "bool",
    "BASE_URL": "str?",
    "PGID": "int",
    "PUID": "int",
    "certfile": "str",
    "keyfile": "str",
    "ssl": "bool"
  },
  "slug": "mealie",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "v1.0.0-beta-3",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:3000]"
}