{
  "apparmor": true,
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "description": "Cloud Commander a file manager for the web with console and editor",
  "devices": [
    "/dev/mmcblk0p8",
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {},
  "ingress": true,
  "ingress_port": 8099,
  "ingress_stream": true,
  "map": [
    "ssl:rw",
    "share:rw",
    "config:rw",
    "media:rw",
    "backup:rw",
    "addons:rw"
  ],
  "name": "Cloudcommander",
  "options": {},
  "panel_icon": "mdi:file-search",
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "8000/tcp": "Web UI port (required)"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "smbv1": "bool?",
    "CUSTOM_OPTIONS": "str?",
    "DROPBOX_TOKEN": "str?"
  },
  "slug": "cloudcommander",
  "upstream": "15.9.11",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/cloudcommander#readme",
  "version": "15.9.11",
  "webui": "http://[HOST]:[PORT:8000]"
}
