{
  "arch": ["aarch64", "amd64", "armv7", "armhf"],
  "boot": "auto",
  "description": "PiwigoÂ is a photo gallery software for the web",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PWG_LOCAL_DIR": "/data"
  },
  "map": ["media:rw", "share:rw", "ssl"],
  "name": "Piwigo",
  "options": {
    "PUID": 1000,
    "PGID": 1000
  },
  "ports": {
    "80/tcp": 81
  },
  "ports_description": {
    "80/tcp": "Web UI port"
  },
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH"],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "TZ": "str?"
  },
  "slug": "piwigo",
  "stage": "experimental",
  "startup": "services",
  "upstream": "12.1.0",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "12.1.0",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]"
}
