{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "web app to manage Two-Factor Authentication",
  "environment": {
    "DB_DATABASE": "/config/database.sqlite",
    "TRUSTED_PROXIES": "*"
  },
  "init": false,
  "map": [
    "addon_config:rw"
  ],
  "name": "2FAuth",
  "ports": {
    "8000/tcp": 8000
  },
  "ports_description": {
    "8000/tcp": "Web interface"
  },
  "schema": {
    "APP_URL": "str",
    "SITE_OWNER": "str?",
    "MAIL_DRIVER": "str?",
    "MAIL_HOST": "str?",
    "MAIL_PORT": "int?",
    "MAIL_USERNAME": "str?",
    "MAIL_PASSWORD": "str?",
    "MAIL_ENCRYPTION": "str?",
    "MAIL_FROM_NAME": "str?",
    "MAIL_FROM_ADDRESS": "str?"
  },
  "slug": "2fauth",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/2fauth",
  "version": "1.1",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8000]"
}
