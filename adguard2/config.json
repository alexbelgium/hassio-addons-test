{
    "name": "Adguard Home",
    "version": "v0.104.3_test",
    "slug": "adguard_home_docker",
    "description": "Free and open source, powerful network-wide ads & trackers blocking DNS server",
    "url": "https://github.com/alexbelgium/hassio-addons/tree/master/adguard",
    "startup": "services",
    "ingress": true,	
    "ingress_port": 0,
    "panel_icon": "mdi:shield-check",
    "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf",
    "i386"
  ],
   "ports": {
      "53/tcp": 53,
      "53/udp": 53,
      "80/tcp": 80,
      "3000/tcp": 3000,
      "67/udp": null,
      "68/tcp": null,
      "68/udp": null,
      "443/tcp": null,
      "853/tcp": null,
      "784/tcp": null,
      "5443/tcp": null,
      "5443/udp": null
    },
   "ports_description": {
      "53/tcp": "DNS port",
      "53/udp": "DNS port",
      "80/tcp": "Web UI",
      "3000/tcp": "Web UI",
      "67/udp": "DHCP server port 1",
      "68/tcp": "DHCP server port 2",
      "68/udp": "DHCP server port 3",
      "443/tcp": "HTTPS/DNS-over-HTTPS server",
      "853/tcp": "NS-over-TLS server",
      "784/tcp": "DNS-over-QUIC server",
      "5443/tcp": "DNSCrypt server port 1",
      "5443/udp": "DNSCrypt server port 2"
    },
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "boot": "auto", 
  "hassio_api": true,
  "discovery": ["adguard"],
  "hassio_role": "default",
  "auth_api": true,
  "host_network": true,
  "map": ["ssl"],
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)?",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "leave_front_door_open": "bool?"
  }
}
