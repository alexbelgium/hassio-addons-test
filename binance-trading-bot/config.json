{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "binance-trading-bot provides a file managing interface within a specified directory and it can be used to upload, delete, preview, rename and edit your files",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/fuse",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "BINANCE_REDIS_HOST": "localhost",
    "BINANCE_REDIS_PORT": "6379",
    "BINANCE_REDIS_PASSWORD": "",
    "PYTHONUNBUFFERED": "1",
    "TRADINGVIEW_LOG_LEVEL": "INFO"
  },
  "map": [
    "config:rw"
  ],
  "name": "binance-trading-bot",
  "options": {
    "BINANCE_MODE": "test"
  },
  "ports": {
    "8080/tcp": 8081,
    "8082/tcp": 8082
  },
  "ports_description": {
    "8080/tcp": "Bot port",
    "8082/tcp": "Web UI port"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "BINANCE_MODE": "list(live|test)"
  },
  "slug": "binance-trading-bot",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "2.23.0-5"
}