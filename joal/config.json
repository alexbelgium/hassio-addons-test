{
    "name": "Joal Desktop test",
    "version": "2.1.24_1",
    "slug": "joal",
    "description": "Joal Desktop docker",
    "url": "https://github.com/alexbelgium/hassio-addons/tree/master/joal",
    "startup": "services",
    "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
   "ports": {
      "1234/tcp": 1234
    },
   "ports_description": {
      "1234/tcp": "Web UI port (required)"
    },
  "map": [
    "share:rw"
  ],
  "webui": "http://[HOST]:[PORT:1234]",
  "boot": "auto", 
  "environment": {
  },
  "options": {
  "joal-conf": "/data/joal-conf",
  "spring.main.web-environment": true,
  "server.port": 8081
	},
  "schema": {
  "joal-conf": "str",
  "spring.main.web-environment": "bool",
  "server.port": "int"
  }
}
