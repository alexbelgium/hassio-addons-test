{
    "name": "Joal Desktop test",
    "version": "2.1.24_3",
    "slug": "joal",
    "description": "Joal Desktop docker",
    "url": "https://github.com/alexbelgium/hassio-addons/tree/master/joal",
    "startup": "services",
    "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
   "ports": {
      "1234/tcp": 1234, 
      "8081/tcp": 8081
    },
   "ports_description": {
      "1234/tcp": "Torrent port",
      "8081/tcp": "Web UI port"
    },
  "map": [
    "share:rw"
  ],
  "webui": "http://[HOST]:[PORT:8081]/joal/ui",
  "boot": "auto", 
  "environment": {
  "joal-conf": "/joal",
  "joal.ui.path.prefix": "/joal", 
  "spring.main.web-environment": true,
  "joal.ui.secret-token": "lrMY24Byhx",
  "joal.ui.path.prefix": "joal",
  },
  "options": {
  "joal-conf": "/joal/options.json",
  "spring.main.web-environment": true,
  "server.port": 8081
	},
  "schema": {
  "joal-conf": "str",
  "spring.main.web-environment": "bool",
  "server.port": "int"
  }
}
