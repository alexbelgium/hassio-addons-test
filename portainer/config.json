{
  "name": "Portainer",
  "version": "2.1.1",
  "upstream": "2.1.1",
  "slug": "portainer",
  "description": "Making Docker Management Easy",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "advanced": true,
  "startup": "services",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf"
  ],
  "ports": {
    "9000/tcp": 9000
  },
  "ports_description": {
    "9000/tcp": "Web UI port",
    "8000/tcp": "Edge Agent Api (Enable when managing remote edge agents)"
  },
  "hassio_api": true,
  "docker_api": true,
  "apparmor": false,
  "full_access": true,
  "map": [
    "ssl"
  ],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:9000]",
  "environment": {
  },
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
	"password": "default"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
	"password": "str"
  }
}
