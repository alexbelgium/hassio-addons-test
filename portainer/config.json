{
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "description": "Manage your Docker environment with ease",
  "docker_api": true,
  "hassio_api": true,
  "ingress": true,
  "ingress_port": 1337,
  "ingress_stream": true,
  "init": false,
  "map": [
    "share:rw",
    "ssl"
  ],
  "name": "Portainer",
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "password": "default"
  },
  "panel_icon": "mdi:docker",
  "ports": {
    "9099/tcp": 9000,
    "8000/tcp": null
  },
  "ports_description": {
    "9099/tcp": "Web UI port",
    "8000/tcp": "Edge Agent Api (Enable when managing remote edge agents)"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "password": "str?"
  },
  "slug": "portainer",
  "startup": "services",
  "upstream": "2.11.0",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "2.11.0",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:9099]"
}
