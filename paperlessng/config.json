{
    "name": "paperless ng",
    "version": "1.0",
    "slug": "paperlessng",
    "description": "",
    "url": "https://github.com/jonaswinkler/paperless-ng",
    "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
    "webui": "http://[HOST]:[PORT:8000]",
    "ports": {
    "8000/tcp": 8000,
    "6379/tcp": 6379        
    },
   "ports_description": {
    "8000/tcp": "Web UI"
    },
  "environment": {
"PAPERLESS_OCR_LANGUAGE="eng",
"DEFAULT_USERNAME="username",
"DEFAULT_EMAIL="address@gmail.com",
"DEFAULT_PASSWORD="password"
  },
  "map": [
    "share:rw",
    "ssl"
  ],
  "options": {
  "UID": 1000,
  "GID": 1000,
  "secretkey": "secretkey",
  "ocr_lang": "eng",
  "username": "username",
  "email": "email@gmail.com",
  "password": "password",
  "consumption_dir": "/share/paperless/consume",
  "data_dir": "/share/paperless/data",
  "media_root": "/share/paperless/media",
  "format": "{created_year}/{correspondent}/{title}",
  "whitelist": "192.168.178.23,www.google.fr"
},
  "schema": {
  "UID": "int?",
  "GID": "int?",
  "secretkey": "str",
  "ocr_lang": "str",
  "username": "str",
  "email": "str",
  "password": "str",
  "consumption_dir": "str",
  "data_dir": "str",
  "media_root": "str",  
  "format": "str",
  "whitelist": "str"
  }
}
