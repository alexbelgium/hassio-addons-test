{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "scan, index and archive all your physical documents",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PGID": "0",
    "PUID": "0"
  },
  "homeassistant": "0.92.0b2",
  "init": false,
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "config:rw"
  ],
  "name": "Paperless NGX",
  "options": {
    "CONFIG_LOCATION": "/config/addons_config/paperless_ng/config.yaml",
    "PGID": 0,
    "PUID": 0
  },
  "ports": {
    "8000/tcp": 8011
  },
  "ports_description": {
    "8000/tcp": "web interface"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "CONFIG_LOCATION": "str",
    "OCRLANG": "str?",
    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "PAPERLESS_OCR_MODE": "list(skip|redo|force|skip_noarchive)?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "slug": "paperless_ng",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "1.8.2-test",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8000]"
}